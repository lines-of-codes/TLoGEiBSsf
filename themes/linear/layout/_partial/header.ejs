<header>
  <div class="site-title">
    <a href="/">
      <%- config.title %>
    </a>
  </div>
  <% if (config.description) { %>
  <div class="site-subtitle">
    <%- config.description %>
  </div>
  <% } %>
  <% if (theme.links.length) { %>
  <p class="links">
    <% theme.links.forEach((item,index)=>{ %>
    <a href="<%- item.link %>">
      <img src="<%- theme.linkiconbase %><%- item.icon %>" alt="<%- item.name %>">
    </a>
    <% }) %>
  </p>
  <% } %>
</header>